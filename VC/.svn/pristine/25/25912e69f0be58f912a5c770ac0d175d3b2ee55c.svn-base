package com.tapi.tcs.vc.webservice.provider.newCancelInvoice.bean;

import javax.xml.bind.annotation.XmlElement;

public class CancelInvoiceRequestBody {
		private String kpzdbs;
		private String docVerCode;//单证类型代码
		private String pressBatchNo;//发票代码
		private String docNum;//发票号码
		private String cancelType;//作废类型0：空白票作废
		private String amount;//合计金额
		private String cancelDate;//作废日期
		private String cancelPeople;//作废人
		private String canceTakeCode;//作废人代码
		private String takeOrgcode;//所属机构
		public String getKpzdbs() {
			return kpzdbs;
		}
		@XmlElement(name = "KPZDBS", required = true)
		public void setKpzdbs(String kpzdbs) {
			this.kpzdbs = kpzdbs;
		}
		public String getDocVerCode() {
			return docVerCode;
		}
		@XmlElement(name = "FPLXDM", required = true)
		public void setDocVerCode(String docVerCode) {
			this.docVerCode = docVerCode;
		}
		public String getPressBatchNo() {
			return pressBatchNo;
		}
		@XmlElement(name = "FPDM", required = true)
		public void setPressBatchNo(String pressBatchNo) {
			this.pressBatchNo = pressBatchNo;
		}
		public String getDocNum() {
			return docNum;
		}
		@XmlElement(name = "FPHM", required = true)
		public void setDocNum(String docNum) {
			this.docNum = docNum;
		}
		public String getCancelType() {
			return cancelType;
		}
		@XmlElement(name = "ZFLX", required = true)
		public void setCancelType(String cancelType) {
			this.cancelType = cancelType;
		}
		public String getAmount() {
			return amount;
		}
		@XmlElement(name = "HJJE", required = true)
		public void setAmount(String amount) {
			this.amount = amount;
		}
		public String getCancelDate() {
			return cancelDate;
		}
		 @XmlElement(name = "ZFRQ", required = true)
		public void setCancelDate(String cancelDate) {
			this.cancelDate = cancelDate;
		}
		public String getCancelPeople() {
			return cancelPeople;
		}
		 @XmlElement(name = "ZFR", required = true)
		public void setCancelPeople(String cancelPeople) {
			this.cancelPeople = cancelPeople;
		}
		public String getCanceTakeCode() {
			return canceTakeCode;
		}
		  @XmlElement(name = "ZFRCODE", required = true)
		public void setCanceTakeCode(String canceTakeCode) {
			this.canceTakeCode = canceTakeCode;
		}
		public String getTakeOrgcode() {
			return takeOrgcode;
		}
		  @XmlElement(name = "INSTCODE", required = true)
		public void setTakeOrgcode(String takeOrgcode) {
			this.takeOrgcode = takeOrgcode;
		}
}
