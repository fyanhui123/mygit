package com.tapi.tcs.vc.webservice.provider.docValidInquiry.bean;

public class DocValidInquiryRequestDTO {

	/** 单证类型代码 */
	private String docVerCode;
	/** 业务号 */
	private String businessNo;
	/** 单证起始流水号 */
	private String docBeginSerialNo;
	/** 单证终止流水号 */
	private String docEndSerialNo;
	/** 操作员 */
	private String operator;
	/** 归属机构 */
	private String comCode;
	/** 业务员 */
	private String userId;
	/** 适用场景标记  0:单证打印核销  1:无线出单 2:单证正本打印销号*/
	private String sceneFlag;
	
	public String getDocVerCode() {
		return docVerCode;
	}
	public void setDocVerCode(String docVerCode) {
		this.docVerCode = docVerCode;
	}
	public String getBusinessNo() {
		return businessNo;
	}
	public void setBusinessNo(String businessNo) {
		this.businessNo = businessNo;
	}
	public String getDocBeginSerialNo() {
		return docBeginSerialNo;
	}
	public void setDocBeginSerialNo(String docBeginSerialNo) {
		this.docBeginSerialNo = docBeginSerialNo;
	}
	public String getDocEndSerialNo() {
		return docEndSerialNo;
	}
	public void setDocEndSerialNo(String docEndSerialNo) {
		this.docEndSerialNo = docEndSerialNo;
	}
	public String getOperator() {
		return operator;
	}
	public void setOperator(String operator) {
		this.operator = operator;
	}
	public String getComCode() {
		return comCode;
	}
	public void setComCode(String comCode) {
		this.comCode = comCode;
	}
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public String getSceneFlag() {
		return sceneFlag;
	}
	public void setSceneFlag(String sceneFlag) {
		this.sceneFlag = sceneFlag;
	}
}
